<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping namespace="Entitades" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Empresa" table="Empresas" schema="dbo">
    <id name="Id" type="Int32">
      <column name="Id" not-null="true" precision="10" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    <property name="Direccion" type="String">
      <column name="Direccion" not-null="false" length="100" sql-type="varchar" />
    </property>
    <property name="Nombre" type="String">
      <column name="Nombre" not-null="true" length="50" sql-type="varchar" />
    </property>
  </class>
</hibernate-mapping>