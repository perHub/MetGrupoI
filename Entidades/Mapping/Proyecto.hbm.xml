<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping namespace="Entidades" assembly="Entidades" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Proyecto" table="Proyectos" schema="dbo">
    <id name="Id" type="Int32">
      <column name="Id" not-null="true" precision="10" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    <property name="Nombre" type="String">
      <column name="Nombre" not-null="true" length="50" sql-type="varchar" />
    </property>
    <many-to-one name="oEmpresa" class="Empresa" lazy="false">
      <column name="IdEmpresa" not-null="true" precision="10" scale="0" sql-type="int" />
    </many-to-one>
  </class>
</hibernate-mapping>